{
  "name": "@r2n/ui-angular",
  "version": "0.1.1",
  "description": "r2n angular component library",
  "author": "Andres Artunduaga",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andres-artunduaga/r2n-ui.git"
  },
  "scripts": {
    "build": "yarn build.ng",
    "build.fesm": "rollup --config ./scripts/rollup.config.js",
    "build.ng": "yarn build.es2015 && yarn build.es5",
    "build.es2015": "ngc -p tsconfig.json && rollup --config ./scripts/rollup.config.js",
    "build.es5": "ngc -p tsconfig.legacy.json && rollup --config ./scripts/rollup.config.legacy.js",
    "lint": "yarn lint.ts",
    "lint.ts": "tslint --project .",
    "lint.fix": "tslint --project . --fix",
    "prerelease": "yarn validate && np prerelease --yolo --any-branch --tag next",
    "test": "echo 'angular no tests yet'",
    "tsc": "tsc -p .",
    "validate": "yarn && yarn lint && yarn test && yarn build"
  },
  "module": "dist/fesm5.js",
  "main": "dist/fesm5.js",
  "types": "dist/core.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@r2n/ui": "0.1.1",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "^12.2.9",
    "@angular/common": "^12.2.9",
    "@angular/compiler": "^12.2.9",
    "@angular/compiler-cli": "^12.2.9",
    "@angular/core": "^12.2.9",
    "@angular/forms": "^12.2.9",
    "@angular/platform-browser": "^12.2.9",
    "@angular/platform-browser-dynamic": "^12.2.9",
    "@angular/router": "^12.2.9",
    "rxjs": "~6.6.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/core": "^12.2.9",
    "@angular-devkit/schematics": "^12.2.9",
    "@angular/common": "^12.2.9",
    "@angular/compiler": "^12.2.9",
    "@angular/compiler-cli": "^12.2.9",
    "@angular/core": "^12.2.9",
    "@angular/forms": "^12.2.9",
    "@angular/platform-browser": "^12.2.9",
    "@angular/platform-browser-dynamic": "^12.2.9",
    "@angular/router": "^12.2.9",
    "@types/node": "^16.10.3",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "rollup": "^2.58.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rxjs": "~6.6.0",
    "tsickle": "^0.34.0",
    "tslint": "^6.1.3",
    "tslint-ionic-rules": "^0.0.21",
    "typescript": "4.3.5",
    "zone.js": "^0.11.4"
  }
}
